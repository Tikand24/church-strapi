{
  "kind": "collectionType",
  "collectionName": "news_letter_subscriptions",
  "info": {
    "singularName": "news-letter-subscription",
    "pluralName": "news-letter-subscriptions",
    "displayName": "News letter subscription"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "statusSub": {
      "type": "enumeration",
      "required": true,
      "default": "active",
      "enum": [
        "active",
        "unsubscribed",
        "pending"
      ]
    },
    "subscribedAt": {
      "type": "datetime",
      "required": true
    },
    "source": {
      "type": "string",
      "required": true
    }
  }
}
